								<UML:Class name="AerodromeSurfaceWind" xmi.id="EAID_6C130866_43F7_4e77_A513_55BB912ECE18" visibility="public" namespace="EAPK_CA480557_2AAC_4a38_A487_DA136C232B0F" isRoot="false" isLeaf="false" isAbstract="false" isActive="false">
