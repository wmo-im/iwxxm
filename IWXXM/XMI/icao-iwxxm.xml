								<UML:Class name="SeaSurfaceState" xmi.id="EAID_1B428F25_F644_4639_A772_833093E2DC64" visibility="public" namespace="EAPK_CA480557_2AAC_4a38_A487_DA136C232B0F" isRoot="false" isLeaf="false" isAbstract="false" isActive="false">
