								<UML:Class name="SPECI" xmi.id="EAID_20DC3A7D_1815_45a2_A924_D744635AA46B" visibility="public" namespace="EAPK_CA480557_2AAC_4a38_A487_DA136C232B0F" isRoot="false" isLeaf="false" isAbstract="false" isActive="false">
