										<UML:Constraint name="CRS requirements for iwxxm:aerodrome">
											<UML:ModelElement.taggedValue>
												<UML:TaggedValue tag="description" value="Pattern ID: TAF.TAF-9&#xA;Description: If a geometry is defined for iwxxm:aerodrome//aixm:AIP with the provision of attribute srsName, attribute srsDimension must equal to 2 and attribute axisLabels must be non-empty&#xA;Assertion: ( if( exists(iwxxm:aerodrome//aixm:ARP) ) then( empty(index-of(iwxxm:aerodrome//aixm:ARP//gml:pos/(ancestor-or-self::*[exists(@srsName)])[last()]/(@srsDimension='2' and exists(@axisLabels)), false())) ) else( true() ) )"/>
												<UML:TaggedValue tag="type" value="Invariant"/>
												<UML:TaggedValue tag="weight" value="8.00"/>
												<UML:TaggedValue tag="status" value="Approved"/>
											</UML:ModelElement.taggedValue>
										</UML:Constraint>
										<UML:Constraint name="If( not(empty(baseForecast)) ) then( empty(@changeIndicator) )">
											<UML:ModelElement.taggedValue>
												<UML:TaggedValue tag="description" value="Pattern ID: TAF.TAF-6&#xA;Description: A non-empty iwxxm:baseForecast should not have @changeIndicator&#xA;Assertion: ( if( exists(iwxxm:baseForecast/*) ) then( empty(iwxxm:baseForecast/iwxxm:MeteorologicalAerodromeForecast/@changeIndicator) ) else( true() ) )"/>
												<UML:TaggedValue tag="type" value="Invariant"/>
												<UML:TaggedValue tag="weight" value="5.00"/>
												<UML:TaggedValue tag="status" value="Approved"/>
											</UML:ModelElement.taggedValue>
										</UML:Constraint>
										<UML:Constraint name="If( not(empty(baseForecast)) ) then( not(empty(surfaceWind)) )">
											<UML:ModelElement.taggedValue>
												<UML:TaggedValue tag="description" value="Pattern ID: TAF.TAF-7&#xA;Description: in a non-empty iwxxm:baseForecast wxxm:surfaceWind is mandatory&#xA;Assertion: ( if( exists(iwxxm:baseForecast/*) ) then( exists(iwxxm:baseForecast/iwxxm:MeteorologicalAerodromeForecast/iwxxm:surfaceWind) ) else( true() ) )"/>
												<UML:TaggedValue tag="type" value="Invariant"/>
												<UML:TaggedValue tag="weight" value="6.00"/>
												<UML:TaggedValue tag="status" value="Approved"/>
											</UML:ModelElement.taggedValue>
										</UML:Constraint>
										<UML:Constraint name="If( not(empty(baseForecast)) and not(@cloudAndVisibilityOK = 'ture') ) then( exists(prevailingVisibility, cloud) )">
											<UML:ModelElement.taggedValue>
												<UML:TaggedValue tag="description" value="Pattern ID: TAF.TAF-8&#xA;Description: In a non-empty iwxxm:baseForecast when @cloudAndVisibilityOK is false or empty iwxxm:prevailingVisibility and iwxxm:cloud are mandatory&#xA;Assertion: ( if( exists(iwxxm:baseForecast/*) and not(iwxxm:baseForecast/iwxxm:MeteorologicalAerodromeForecast/@cloudAndVisibilityOK = 'true') ) then( exists(iwxxm:baseForecast/iwxxm:MeteorologicalAerodromeForecast/iwxxm:prevailingVisibility) and exists(iwxxm:baseForecast/iwxxm:MeteorologicalAerodromeForecast/iwxxm:cloud) ) else( true() ) )"/>
												<UML:TaggedValue tag="type" value="Invariant"/>
												<UML:TaggedValue tag="weight" value="7.00"/>
												<UML:TaggedValue tag="status" value="Approved"/>
											</UML:ModelElement.taggedValue>
										</UML:Constraint>
										<UML:Constraint name="Mandatory items in a CANCELLATION report">
											<UML:ModelElement.taggedValue>
												<UML:TaggedValue tag="description" value="Pattern ID: TAF.TAF-4&#xA;Description: A CANCELLATION report should have the appropriate elements filled including iwxxm:issueTime, iwxxm:aerodrome, iwxxm:cancelledReportValidPeriod. Elements iwxxm:validPeriod, iwxxm:baseForecast and iwxxm:changeForecast are absent&#xA;Assertion: ( if( (@isCancelReport = 'true') and (string-length(@translationFailedTAC) eq 0) and (@permissibleUsage = 'OPERATIONAL') ) then( exists(iwxxm:issueTime) and exists(iwxxm:aerodrome) and empty(iwxxm:validPeriod) and exists(iwxxm:cancelledReportValidPeriod) and empty(iwxxm:baseForecast) and empty(iwxxm:changeForecast) ) else( true() ) )"/>
												<UML:TaggedValue tag="type" value="Invariant"/>
												<UML:TaggedValue tag="weight" value="3.00"/>
												<UML:TaggedValue tag="status" value="Approved"/>
											</UML:ModelElement.taggedValue>
										</UML:Constraint>
										<UML:Constraint name="Mandatory items in a Nil report">
											<UML:ModelElement.taggedValue>
												<UML:TaggedValue tag="description" value="Pattern ID: TAF.TAF-3&#xA;Description: A 'Nil' report should have appropriately filled elements including iwxxm:issueTime, iwxxm:aerodrome, iwxxm:validPeriod (missing), iwxxm:cancelledReportValidPeriod (missing), iwxxm:baseForecast (empty with nilReason) and iwxxm:changeForecast (missing)&#xA;Assertion: ( if( empty(iwxxm:baseForecast/*) and (iwxxm:baseForecast/@nilReason = 'http://codes.wmo.int/common/nil/missing') and (string-length(@translationFailedTAC) eq 0) and (@permissibleUsage = 'OPERATIONAL')  ) then( exists(iwxxm:issueTime) and exists(iwxxm:aerodrome) and empty(iwxxm:validPeriod) and empty(iwxxm:cancelledReportValidPeriod) and empty(iwxxm:changeForecast) ) else( true() ) )"/>
												<UML:TaggedValue tag="type" value="Invariant"/>
												<UML:TaggedValue tag="weight" value="2.00"/>
												<UML:TaggedValue tag="status" value="Approved"/>
											</UML:ModelElement.taggedValue>
										</UML:Constraint>
										<UML:Constraint name="Mandatory items in a non-operational report or a report that failed translation">
											<UML:ModelElement.taggedValue>
												<UML:TaggedValue tag="description" value="Pattern ID: TAF.TAF-2&#xA;Description: A non-operational report or a report that failed translation should have as a minimum appropriately filled elements including iwxxm:issueTime, iwxxm:aerodrome and (iwxxm:validPeriod or iwxxm:cancelledReportValidPeriod)&#xA;Assertion: ( if( (string-length(@translationFailedTAC) gt 0) or (@permissibleUsage = 'NON-OPERATIONAL') ) then( exists(iwxxm:issueTime) and exists(iwxxm:aerodrome) and ((@isCancelReport = 'true' and exists(iwxxm:cancelledReportValidPeriod)) or (not(@isCancelReport = 'true') and exists(iwxxm:validPeriod))) ) else( true() ) )"/>
												<UML:TaggedValue tag="description" value="Pattern ID: TAF.TAF-2&#xA;Description: A non-operational report or a report that failed translation should have as a minimum appropriately filled elements including iwxxm:issueTime, iwxxm:aerodrome and (iwxxm:validPeriod or iwxxm:cancelledReportValidPeriod)&#xA;Assertion: ( if( (string-length(@translationFailedTAC) gt 0) or (@permissibleUsage = 'NON-OPERATIONAL') ) then( exists(iwxxm:issueTime) and exists(iwxxm:aerodrome) and ((@isCancelReport = 'true' and exists(iwxxm:cancelledReportValidPeriod)) or (not(@isCancelReport = 'true') and exists(iwxxm:validPeriod))) ) else( true() ) )"/>
												<UML:TaggedValue tag="type" value="Invariant"/>
												<UML:TaggedValue tag="weight" value="1.00"/>
												<UML:TaggedValue tag="status" value="Approved"/>
											</UML:ModelElement.taggedValue>
										</UML:Constraint>
										<UML:Constraint name="Mandatory items in an ordinary report">
											<UML:ModelElement.taggedValue>
												<UML:TaggedValue tag="description" value="Pattern ID: TAF.TAF-5&#xA;Description: An ordinary report should have appropriately filled elements including iwxxm:issueTime, iwxxm:aerodrome, iwxxm:validPeriod, iwxxm:cancelledReportValidPeriod (missing) and iwxxm:baseForecast&#xA;Assertion: ( if( (iwxxm:baseForecast/@nilReason != 'http://codes.wmo.int/common/nil/missing' and (empty(@isCancelReport) or @isCancelReport = 'false')) and (string-length(@translationFailedTAC) eq 0) and (@permissibleUsage = 'OPERATIONAL') ) then( exists(iwxxm:issueTime) and exists(iwxxm:aerodrome) and exists(iwxxm:validPeriod) and empty(iwxxm:cancelledReportValidPeriod) and exists(iwxxm:baseForecast) ) else( true() ) )"/>
												<UML:TaggedValue tag="type" value="Invariant"/>
												<UML:TaggedValue tag="weight" value="4.00"/>
												<UML:TaggedValue tag="status" value="Approved"/>
											</UML:ModelElement.taggedValue>
										</UML:Constraint>
										<UML:Constraint name="Nil and cancellation reports are mutually exclusive">
											<UML:ModelElement.taggedValue>
												<UML:TaggedValue tag="description" value="Pattern ID: TAF.TAF-1&#xA;Description: Attributes isNilReport and isCancelReport cannot be true at the same time&#xA;Assertion: ( not((@isNilReport = 'true') and (@isCancelReport = 'true')) )"/>
												<UML:TaggedValue tag="type" value="Invariant"/>
												<UML:TaggedValue tag="weight" value="0.00"/>
												<UML:TaggedValue tag="status" value="Approved"/>
											</UML:ModelElement.taggedValue>
										</UML:Constraint>
