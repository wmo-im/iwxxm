								<UML:Class name="VisualRangeTendency" xmi.id="EAID_26EE59C8_C3B1_41af_B550_25FD501B56FD" visibility="public" namespace="EAPK_CA480557_2AAC_4a38_A487_DA136C232B0F" isRoot="false" isLeaf="false" isAbstract="false" isActive="false">
