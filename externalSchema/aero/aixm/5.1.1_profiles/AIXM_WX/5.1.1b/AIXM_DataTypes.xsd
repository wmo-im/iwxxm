	<simpleType name="AlphaType">
		<restriction base="string">
			<pattern value="[A-Z]*"/>
		</restriction>
	</simpleType>
	<simpleType name="AlphanumericType">
		<restriction base="string">
			<pattern value="([A-Z]|\d)*"/>
